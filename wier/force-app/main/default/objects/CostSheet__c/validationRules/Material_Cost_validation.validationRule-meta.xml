<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Material_Cost_validation</fullName>
    <active>true</active>
    <description>Material_Cost validation</description>
    <errorConditionFormula>Material_Cost__c &lt; 0</errorConditionFormula>
    <errorMessage>Material cost must be greater than or equal to zero.</errorMessage>
</ValidationRule>
