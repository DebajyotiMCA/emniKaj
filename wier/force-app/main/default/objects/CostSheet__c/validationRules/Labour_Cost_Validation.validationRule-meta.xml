<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Labour_Cost_Validation</fullName>
    <active>true</active>
    <description>Lab our Cost Validation</description>
    <errorConditionFormula>Labour_Cost__c &lt; 0</errorConditionFormula>
